<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xinchen.mvc.dao.SellerShowDao">
    <select id="queryById" parameterType="long" resultType="SellerShow">
        SELECT *
        FROM kmu_sellershow
        WHERE id = #{id}
    </select>

    <select id="queryAll"  resultType="SellerShow">
        SELECT *
        FROM kmu_sellershow
    </select>
</mapper>